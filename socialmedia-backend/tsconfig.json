{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "src",
    "paths": {
      "@config/*": [
        "config/*"
      ],
      "@routes/*": [
        "routes/*"
      ],
      "@models/*": [
        "models/*"
      ],
      "@swagger/*": [
        "swagger/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      // "shared": [
      //   "../shared"
      // ],
    }
  }
}